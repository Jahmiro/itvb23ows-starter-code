pipeline {
    agent {label '!windows'}
    stages {
         stage('build') {
            steps {
                sh 'echo "Building..."'
            }
        }
        stage('SonarQube') {
            steps {
                script { scannerHome = tool 'SonarQube Scanner' }
                withSonarQubeEnv('SonarQube') {
                    sh "${scannerHome}/bin/sonar-scanner \
                        -Dsonar.projectKey=ows-sonarqube"
                }
            }
        }
    }
}


