pipeline {
    agent {label '!windows'}
    stages {
         stage('Build') {
            steps {
                sh 'Building..." '
            }
        }
        stage('SonarQube') {
            steps {
                script { scannerHome = tool 'sonar-scanner' }
                withSonarQubeEnv('SonarQube') {
                    sh "${scannerHome}/bin/sonar-scanner \
                        -Dsonar.projectKey=sqp_21d3d89ae9208ebc2ad16122f211bc8e43880f82"
                }
            }
        }
    }
}
